{% block html_head %}
	{% include 'html-header.twig' %}
	{% block head %}
	{% endblock %}
	</head>
{% endblock %}

	<body class="{{body_class}}" data-template="base.twig">
		<header class="site-header">
			{% block header %}
				<div class="wrapper">
					<img src="site-logo-link" href="{{site.link}}" rel="home">{{site.name}}>
				</div>
			{% endblock %}
		</header>

		<main role="main" class="site-main">
			<div class="main__plus-nav">
			{% include 'partials/nav.twig' %}
				<div class="wrapper {{sidebar_class}}">
					{% block content %}
						Sorry, no content
					{% endblock %}
				</div>
			{% if sidebar %}
				<aside class="main-sidebar">
					{{sidebar}}
				</aside>
			{% endif %}
			</div>
		</main>

		<footer class="site-footer">
			{% block footer %}
				<div class="wrapper">
					{% include 'footer.twig' %}
				</div>
			{% endblock %}
		</footer>
		{{ function('wp_footer') }}

	</body>
</html>
